{% assign listed_teams = site.data.students.teams %}

<ul class="student-teams">
    {% assign cyber_teams = listed_teams | where_exp: "team", "team.group == 'Cyber'" %}
    {% for cyber_team in cyber_teams %}
        {% include _templates/list/element/_student_team.liquid team=cyber_team %}
    {% endfor %}
</ul>
<ul class="student-teams">
    {% assign dev_teams = listed_teams | where_exp: "team", "team.group == 'Dev'" %}
    {% for dev_team in dev_teams %}
        {% include _templates/list/element/_student_team.liquid team=dev_team %}
    {% endfor %}
</ul>
